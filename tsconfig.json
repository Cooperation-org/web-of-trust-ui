{
  "compilerOptions": {
    "moduleResolution": "node",   // Ensures modules like MUI are resolved correctly
    "baseUrl": ".",               // Enables relative imports
    "paths": {                    // Optional: Add specific aliases
      "@mui/*": ["node_modules/@mui/*"]
    },
    "jsx": "react-jsx",           // Required for React projects
    "target": "ES6",
    "module": "ES6",
    "declaration": true,
    "strict": true,
    "outDir": "dist"
  }
}
